		<property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="folderId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="logReason" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>