<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.3.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_3_0.dtd">

<service-builder dependency-injector="ds"
	package-path="com.everis.rec.validation.log">
	<namespace>VALIDATIONLOG</namespace>
	<entity local-service="true" name="ValidationLog"
		remote-service="true" table="validation_log">

		<!-- PK fields -->

		<column name="validationLogId" primary="true" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="groupId" type="long" />
		<column name="createDate" type="Date" />

		<!-- Other fields -->

		<column name="fileName" type="String" />
		<column name="uploadedBy" type="String" />
		<column name="uploadedFrom" type="String" />
		<column name="folderId" type="long" />
		<column name="logReason" type="String" />

		<!-- Order -->

		<order by="asc">
			<order-column name="validationLogId" />
		</order>


	</entity>
</service-builder>